
Процедура СправочникСписокВыбор(Элемент, ВыбраннаяСтрока, Колонка, СтандартнаяОбработка)
	
	Если  ВыбраннаяСтрока.ЭтоГРуппа=Ложь ТОгда   
		ОтметкаАудита(ВыбраннаяСтрока.ПолучитьОбъект(),ВыбраннаяСтрока,ВыбраннаяСтрока.ПутьКФайлу);

		//sd61120
		ЭтоОбработка = Ложь;
		ЭтоОтчет = Ложь;
		Если Найти(ВыбраннаяСтрока.ПутьКФайлу,".epf")<>0 тогда
			ВнОбр= ВнешниеОбработки.ПолучитьФорму(ВыбраннаяСтрока.ПутьКФайлу);  
			ЭтоОбработка = Истина;
		ИначеЕсли  Найти(ВыбраннаяСтрока.ПутьКФайлу,".erf")<>0 тогда
			ВнОбр= ВнешниеОтчеты.ПолучитьФорму(ВыбраннаяСтрока.ПутьКФайлу); 
			ЭтоОтчет = Истина;
		КонецЕсли;
		Попытка
			Если Не ВнОбр = Неопределено Тогда
				ВнОбр.Открыть();
			Иначе 
				ВнешнийОтчет = ВнешниеОбработки.Создать(ВыбраннаяСтрока.ПутьКФайлу);
				Если ЭтоОбработка Тогда
					ВнешняяОбработка = ВнешниеОбработки.Создать(ВыбраннаяСтрока.ПутьКФайлу);
				Иначе
					ВнешнийОтчет = ВнешниеОтчеты.Создать(ВыбраннаяСтрока.ПутьКФайлу);
					Форма = ВнешнийОтчет.ПолучитьФорму();
					Если Форма <> Неопределено Тогда
						Форма.Открыть();
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
		Исключение
			
			Предупреждение("Выбранный файл не является "+?(ВнешнийОтчет = Истина,"внешней обработкой","внешним отчетом") +".
			|Либо "+?(ВнешнийОтчет = Истина,"данная обработка","данный отчет") +" не предназначена для
			|запуска в этой конфигурации.");
			
		КонецПопытки;

		СтандартнаяОбработка=Ложь;
	КонецЕсли; 
КонецПроцедуры

