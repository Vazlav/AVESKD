
Процедура ДействияФормыАЧоБылоТо(Кнопка)
	ТекЗапись=ЭлементыФормы.СправочникСписок.ТекущаяСтрока;
	
	Если Найти(ТекЗапись.Объект,"ОтчетОбъект")<>0 и Найти(ТекЗапись.Объект,"Внешний")=0 Тогда
		ФормаОбъекта=Отчеты[СтрЗаменить(ТекЗапись.Объект,"ОтчетОбъект.","")].ПолучитьФорму();
	ИначеЕсли  Найти(ТекЗапись.Объект,"ОбработкаОбъект")<>0 и Найти(ТекЗапись.Объект,"Внешний")=0 Тогда
		ФормаОбъекта=Обработки[СтрЗаменить(ТекЗапись.Объект,"ОбработкаОбъект.","")].ПолучитьФорму();	
	ИначеЕсли  Найти(ТекЗапись.Объект,"ВнешнийОтчетОбъект")<>0 Тогда
		ФормаОбъекта=ВнешниеОтчеты.Создать(СтрПолучитьСтроку(ТекЗапись.Комментарий,1)).ПолучитьФорму();  // в первой строке коммнтария сидит ссылка на файл
	ИначеЕсли  Найти(ТекЗапись.Объект,"ВнешняяОбработкаОбъект")<>0 Тогда
		ФормаОбъекта=ВнешниеОбработки.Создать(СтрПолучитьСтроку(ТекЗапись.Комментарий,1)).ПолучитьФорму(); // в первой строке коммнтария сидит ссылка на файл
	КонецЕсли;   
	СоответствиеПараметров=ТекЗапись.Значение.Получить();// соответствие
	
	Для Каждого ЭлемСоответствия из СоответствиеПараметров Цикл
		
		ФормаОбъекта.ЭлементыФормы[ЭлемСоответствия.Ключ].Значение=ЭлемСоответствия.Значение;
	КонецЦикла;  
	
	ФормаОбъекта.Открыть();
	
	ОчиститьСообщения();
	
	Сообщить(ТекЗапись.Комментарий);
КонецПроцедуры
